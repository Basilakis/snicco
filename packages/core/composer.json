{
  "name": "sniccowp/core",
  "description": "The core of the sniccowp framework",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-mbstring": "*",
    "webmozart/assert": "^1.10",
    "sniccowp/str-arr": "dev-master",
    "sniccowp/event-dispatcher": "dev-master",
    "symfony/finder": "^5.0-stable",
    "psr/container": "^1.0.0",
    "psr/log": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Core\\": "src"
    },
    "files": [
      "src/Utils/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Core\\": "tests"
    }
  }
}