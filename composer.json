            "Snicco\\Bridge\\SignedUrlPsr16\\Tests\\": "src/Snicco/Bridge/signed-url-psr16/tests",
            "Snicco\\Bridge\\SingedUrlMysqli\\Tests\\": "src/Snicco/Bridge/signed-url-mysqli/tests",
            "Snicco\\Bridge\\SingedUrlPDO\\Tests\\": "src/Snicco/Bridge/signed-url-pdo/tests",
            "Snicco\\Bundle\\BetterWPMail\\Tests\\": "src/Snicco/Bundle/better-wp-mail-bundle/tests",
            "Snicco\\Bundle\\Encryption\\Tests\\": "src/Snicco/Bundle/encryption-bundle/tests",
            "Snicco\\Bundle\\Session\\Tests\\": "src/Snicco/Bundle/session-bundle/tests",
            "Snicco\\Bundle\\SignedUrl\\Tests\\": "src/Snicco/Bundle/signed-url-bundle/tests",
