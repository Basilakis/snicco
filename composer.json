        "phpunit/phpunit": "^9.5",
        "sniccowp/pimple-bridge": "dev-master",
        "symplify/monorepo-builder": "^9.4"
    },
    "autoload": {
        "files": [
            "src/Snicco/Bundle/session-bundle/src/functions.php",
            "src/Snicco/Component/core/src/Utils/functions.php",
            "src/Snicco/Component/event-dispatcher/src/functions.php"
        ],
        "psr-4": {
            "Snicco\\BladeBundle\\": "src/Snicco/Bundle/blade-bundle/src",
            "Snicco\\BladeBundle\\Eloquent\\": "src/Snicco/Bundle/eloquent-bundle/src",
            "Snicco\\Bridge\\Blade\\": "src/Snicco/Bridge/blade/src",
            "Snicco\\Bridge\\IlluminateContainer\\": "src/Snicco/Bridge/illuminate-container/src",
            "Snicco\\Bridge\\Pimple\\": "src/Snicco/Bridge/pimple/src",
            "Snicco\\Bridge\\SessionPsr16\\": "src/Snicco/Bridge/session-psr16/src",
            "Snicco\\Bridge\\SessionWP\\": "src/Snicco/Bridge/session-wp/src",
            "Snicco\\Bridge\\SignedUrlMiddleware\\": "src/Snicco/Bridge/signed-url-middleware/src",
