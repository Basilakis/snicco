{
    "name": "sniccowp/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "akrabat/ip-address-middleware": "^2.0",
        "bacon/bacon-qr-code": "^2.0",
        "bjeavons/zxcvbn-php": "^1.0",
        "defuse/php-encryption": "^2.0.0",
        "illuminate/container": "^8.0.0",
        "illuminate/database": "^v8.0.0",
        "illuminate/view": "^v8.0.0",
        "lucatume/wp-browser": "^3.0.0",
        "mockery/mockery": "^1.4.2",
        "nesbot/carbon": "^2.31",
        "nikic/fast-route": "1.3.0",
        "opis/closure": "3.6.2",
        "paragonie/constant_time_encoding": "^2.4",
        "phpunit/phpunit": "^9.5",
        "pimple/pimple": "^3.0.0",
        "pragmarx/google2fa": "8.0.0",
        "psr/container": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "psr/http-server-middleware": "^1.0.0",
        "psr/log": "^1.0.0",
        "respect/validation": "^2.0.0",
        "roots/wp-password-bcrypt": "dev-master",
        "symfony/finder": "^5.0-stable",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "adriansuter/php-autoload-override": "^1.2",
        "codeception/codeception": "4.1.24",
        "fakerphp/faker": "^1.0.0-stable",
        "filp/whoops": "^2.2",
        "nyholm/psr7": "^1.0.0",
        "nyholm/psr7-server": "^1.0-stable",
        "symplify/monorepo-builder": "^9.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "packages/better-wp-hooks/src/functions.php",
            "packages/core/src/Utils/functions.php",
            "packages/session-bundle/src/functions.php"
        ],
        "psr-4": {
            "Snicco\\Auth\\": "packages/auth/src",
            "Snicco\\BladeBundle\\": "packages/blade-bundle/src",
            "Snicco\\Blade\\": "packages/blade/src",
            "Snicco\\Core\\": "packages/core/src",
            "Snicco\\Database\\": "packages/eloquent/src",
            "Snicco\\DefuseEncryption\\": "packages/defuse-php-encryption-bundle/src",
            "Snicco\\EloquentBundle\\": "packages/eloquent-bundle/src",
            "Snicco\\EventDispatcher\\": "packages/better-wp-hooks/src",
            "Snicco\\Illuminate\\": "packages/illuminate-container-bridge/src",
            "Snicco\\MailBundle\\": "packages/better-wp-mail-bundle/src",
            "Snicco\\Mail\\": "packages/better-wp-mail/src",
            "Snicco\\PimpleContainer\\": "packages/pimple-container-bridge/src",
            "Snicco\\SessionBundle\\": "packages/session-bundle/src",
            "Snicco\\Session\\": "packages/session/src",
            "Snicco\\SignedUrlBundle\\": "packages/signed-url-bundle/src",
            "Snicco\\SignedUrlMiddleware\\": "packages/signed-url-middleware/src",
            "Snicco\\SignedUrlWP\\": [
                "packages/signed-url-wp-bundle/src",
                "packages/signed-url-wp/src"
            ],
            "Snicco\\SignedUrl\\": [
                "packages/magic-links/src",
                "packages/signed-url/src"
            ],
            "Snicco\\Support\\": "packages/support/src",
            "Snicco\\Testing\\": "packages/testing/src",
            "Snicco\\Validation\\": "packages/validation-bundle/src",
            "Snicco\\ViewBundle\\": "packages/view-bundle/src",
            "Snicco\\View\\": "packages/view/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Auth\\": "packages/auth/tests",
            "Tests\\BetterWPHooks\\": "packages/better-wp-hooks/tests",
            "Tests\\BetterWPMailBundle\\": "packages/better-wp-mail-bundle/tests",
            "Tests\\BetterWPMail\\": "packages/better-wp-mail/tests",
            "Tests\\BladeBundle\\": "packages/blade-bundle/tests",
            "Tests\\Blade\\": "packages/blade/tests",
            "Tests\\Codeception\\": "codeception",
            "Tests\\Core\\": "packages/core/tests",
            "Tests\\Database\\": "packages/eloquent/tests",
            "Tests\\DefuseEncryption\\": "packages/defuse-php-encryption-bundle/tests",
            "Tests\\EloquentBundle\\": "packages/eloquent-bundle/tests",
            "Tests\\IllumianteContainerBridge\\": "packages/illuminate-container-bridge/tests",
            "Tests\\PimpleContainer\\": "packages/pimple-container-bridge/tests",
            "Tests\\SessionBundle\\": "packages/session-bundle/tests",
            "Tests\\Session\\": "packages/session/tests",
            "Tests\\SignedUrlBundle\\": "packages/signed-url-bundle/tests",
            "Tests\\SignedUrlMiddleware\\": "packages/signed-url-middleware/tests",
            "Tests\\SignedUrlWP\\": [
                "packages/signed-url-wp-bundle/tests",
                "packages/signed-url-wp/tests"
            ],
            "Tests\\SignedUrl\\": [
                "packages/magic-links/tests",
                "packages/signed-url/tests"
            ],
            "Tests\\Testing\\": "packages/testing/tests",
            "Tests\\Validation\\": "packages/validation-bundle/tests",
            "Tests\\ViewBundle\\": "packages/view-bundle/tests",
            "Tests\\View\\": "packages/view/tests"
        }
    },
    "replace": {
        "sniccowp/auth": "self.version",
        "sniccowp/better-wp-hooks": "self.version",
        "sniccowp/better-wp-mail": "self.version",
        "sniccowp/better-wp-mail-bundle": "self.version",
        "sniccowp/blade": "self.version",
        "sniccowp/blade-bundle": "self.version",
        "sniccowp/core": "self.version",
        "sniccowp/defuse-php-encryption-bundle": "self.version",
        "sniccowp/eloquent": "self.version",
        "sniccowp/eloquent-bundle": "self.version",
        "sniccowp/illuminate-container-brige": "self.version",
        "sniccowp/pimple-container-brige": "self.version",
        "sniccowp/session": "self.version",
        "sniccowp/session-bundle": "self.version",
        "sniccowp/signed-url": "self.version",
        "sniccowp/signed-url-bundle": "self.version",
        "sniccowp/signed-url-middleware": "self.version",
        "sniccowp/signed-url-wp": "self.version",
        "sniccowp/signed-url-wp-bundle": "self.version",
        "sniccowp/support": "self.version",
        "sniccowp/testing": "self.version",
        "sniccowp/validation-bundle": "self.version",
        "sniccowp/view": "self.version",
        "sniccowp/view-bundle": "self.version"
    },
    "scripts": {
        "failed-tests": "vendor/bin/codecept run -g failed",
        "unit": "vendor/bin/codecept run -s integration",
        "integration": "vendor/bin/codecept run -s unit",
        "tests": [
            "@integration",
            "@unit"
        ],
        "merge": [
            "vendor/bin/monorepo-builder merge",
            "composer dump-autoload"
        ],
        "deptrac": "php /usr/local/bin/deptrac"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
