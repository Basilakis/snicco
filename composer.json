{
    "name": "sniccowp/sniccowp",
    "description": "The SniccoWP framework",
    "license": "mixed",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "defuse/php-encryption": "^2.0.0",
        "illuminate/container": "^8.0.0",
        "illuminate/database": "^v8.0.0",
        "illuminate/view": "^v8.0.0",
        "nikic/fast-route": "1.3.0",
        "paragonie/constant_time_encoding": "^2.4",
        "pimple/pimple": "^3.0.0",
        "psr/container": "^1.0.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0.0",
        "psr/http-server-middleware": "^1.0.0",
        "psr/log": "^1.0.0",
        "psr/simple-cache": "^1.0.0",
        "symfony/finder": "^5.0-stable",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "adriansuter/php-autoload-override": "^1.2",
