{
  "name": "sniccowp/event-dispatcher",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/event-dispatcher": "^1.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "pimple/pimple": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\EventDispatcher\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\EventDispatcher\\Tests\\": "tests"
    }
  },
  "suggest": {
    "phpunit/phpunit": "To use the assertions on TestableEventDispatcher [^9.5]"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}