{
  "name": "sniccowp/wp-object-cache-psr16",
  "description": "Allows you to use the wp object cache in libraries that require a Psr16 CacheInterface",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/simple-cache": "^1.0.0",
    "sniccowp/scopable-wp": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "cache/integration-tests": "0.17.0",
    "lucatume/wp-browser": "3.0.22"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\WPObjectCachePsr16\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\WPObjectCachePsr16\\Tests\\": "tests"
    }
  },
  "provide": {
    "psr/simple-cache": "^1.0.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}