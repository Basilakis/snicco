{
    "name": "sniccowp/better-wp-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "cache/simple-cache-bridge": "^1.2.0",
        "cache/tag-interop": "^1.1",
        "cache/taggable-cache": "1.2.0",
        "psr/cache": "^1.0.0",
        "psr/simple-cache": "^1.0.0",
        "sniccowp/better-wp-api": "dev-master"
    },
    "require-dev": {
        "cache/integration-tests": "0.17.0",
        "lucatume/wp-browser": "~3.1.4",
        "phpunit/phpunit": "9.5.13"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Component\\BetterWPCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Component\\BetterWPCache\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
