{
  "name": "sniccowp/eloquent",
  "description": "Use the Laravels Eloquent ORM in WordPress. Fully tested with Eloquent 8.x",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-mysqli": "*",
    "illuminate/database": "^v8.0.0",
    "sniccowp/scopable-wp": "dev-master"
  },
  "require-dev": {
    "fakerphp/faker": "^1.0.0-stable",
    "phpunit/phpunit": "^9.5",
    "lucatume/wp-browser": "3.0.22",
    "mockery/mockery": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\Eloquent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\Eloquent\\Tests\\": "tests"
    }
  }
}