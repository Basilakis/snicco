{
  "name": "sniccowp/psr-7-error-handler",
  "description": "A powerful and customizable error handler for psr7 apps.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/log": "^1.0.0",
    "psr/http-message": "^1.0.0",
    "psr/http-factory": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "nyholm/psr7": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\Psr7ErrorHandler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\Psr7ErrorHandler\\Tests\\": "tests"
    }
  },
  "bin": [
    "bin/generate-signed-url-secret"
  ]
}