{
  "name": "sniccowp/session",
  "description": "A custom session implementation that is save to use in distributed WordPress code.",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "sniccowp/testable-clock": "dev-master",
    "sniccowp/str-arr": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4.0",
    "cache/array-adapter": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\Session\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\Session\\Tests\\": "tests",
      "Snicco\\Component\\Session\\Testing\\": "testing"
    }
  }
}