{
  "name": "sniccowp/http-routing",
  "description": "The HTTP and routing component of the sniccowp framework",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "ext-filter": "*",
    "ext-mbstring": "*",
    "psr/container": "^1.1",
    "psr/http-message": "^1.0.0",
    "psr/http-server-middleware": "^1.0.0",
    "psr/http-server-handler": "^1.0.0",
    "psr/http-factory": "^1.0.0",
    "sniccowp/str-arr": "dev-master",
    "sniccowp/psr7-error-handler": "dev-master",
    "nikic/fast-route": "1.3.0",
    "webmozart/assert": "^1.10",
    "webimpress/safe-writer": "^2.2"
  },
  "require-dev": {
    "nyholm/psr7": "^1.0.0",
    "pimple/pimple": "^3.4"
  },
  "suggest": {
    "nyholm/psr7": "To use as a fast PSR7 implementation."
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Component\\HttpRouting\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Component\\HttpRouting\\Tests\\": "tests",
      "Snicco\\Component\\HttpRouting\\Testing\\": "testing"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}