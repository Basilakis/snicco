{
  "name": "sniccowp/payload-middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "sniccowp/http-runner": "dev-master",
    "webmozart/assert": "^1.10",
    "sniccowp/psr7-error-handler": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Middleware\\Payload\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Middleware\\Payload\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}