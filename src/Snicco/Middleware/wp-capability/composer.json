{
  "name": "sniccowp/wp-capapility-middleware",
  "description": "Provides a middleware for sniccowp/http-runner that uses the WP capability system.",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "sniccowp/http-runner": "dev-master",
    "sniccowp/psr7-error-handler": "dev-master",
    "sniccowp/scopable-wp": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Middleware\\WPCap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Middleware\\WPCap\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}