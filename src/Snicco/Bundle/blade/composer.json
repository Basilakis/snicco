    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "sniccowp/templating": "dev-master",
    "sniccowp/templating-bundle": "dev-master",
    "sniccowp/blade-bridge": "dev-master",
    "sniccowp/kernel": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "lucatume/wp-browser": "~3.1.3",
