{
  "name": "sniccowp/debug-bundle",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "sniccowp/kernel": "dev-master",
    "sniccowp/http-routing-bundle": "dev-master",
    "sniccowp/psr7-error-handler": "dev-master",
    "filp/whoops": "^2.14"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "sniccwp/testing-bundle": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bundle\\Debug\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bundle\\Debug\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
