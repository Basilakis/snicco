{
  "name": "sniccowp/session-bundle",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "sniccowp/kernel": "dev-master",
    "sniccowp/session": "dev-master",
    "sniccowp/session-wp": "dev-master",
    "sniccowp/session-psr16": "dev-master",
    "sniccowp/better-wp-hooks": "dev-master",
    "sniccowp/better-wp-hooks-bundle": "dev-master",
    "sniccowp/event-dispatcher": "dev-master",
    "sniccowp/http-routing": "dev-master",
    "sniccowp/http-routing-bundle": "dev-master",
    "sniccowp/better-wpdb": "dev-master",
    "sniccowp/better-wp-api": "dev-master",
    "sniccowp/better-wpdb-bundle": "dev-master",
    "sniccowp/better-wp-cache": "dev-master",
    "sniccowp/better-wp-cache-bundle": "dev-master",
    "sniccowp/encryption-bundle": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "nyholm/psr7": "^1.0.0",
    "sniccowp/testing-bundle": "dev-master",
    "sniccowp/testable-clock": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bundle\\Session\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bundle\\Session\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
