{
  "name": "sniccowp/templating-bundle",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "sniccowp/templating": "dev-master",
    "sniccowp/kernel": "dev-master",
    "sniccowp/psr7-error-handler": "dev-master",
    "sniccowp/http-routing": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "sniccowp/testing-bundle": "dev-master",
    "sniccowp/http-routing-bundle": "dev-master",
    "sniccowp/better-wp-hooks-bundle": "dev-master",
    "nyholm/psr7": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bundle\\Templating\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bundle\\Templating\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
