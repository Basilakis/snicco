{
  "name": "sniccowp/session-bundle",
  "description": "Integrates sniccowp/session with the sniccowp/core",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "sniccowp/core": "dev-master",
    "sniccowp/session": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bundle\\Session\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bundle\\Session\\Tests\\": "tests"
    }
  }
}