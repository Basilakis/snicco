{
    "name": "sniccowp/http-routing-bundle",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "sniccowp/kernel": "dev-master",
        "sniccowp/http-routing": "dev-master",
        "sniccowp/better-wp-hooks-bundle": "dev-master",
        "sniccowp/event-dispatcher": "dev-master",
        "sniccowp/psr7-error-handler": "dev-master",
        "sniccowp/str-arr": "dev-master",
        "sniccowp/better-wp-api": "dev-master",
        "nyholm/psr7-server": "^1.0.2",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "psr/log": "^1.1.1",
        "webimpress/safe-writer": "^2.2",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.13",
        "lucatume/wp-browser": "~3.1.4",
        "sniccowp/testing-bundle": "dev-master",
        "sniccowp/pimple-bridge": "dev-master",
        "nyholm/psr7": "^1.0.0",
        "guzzlehttp/psr7": "^2.1",
        "laminas/laminas-diactoros": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Bundle\\HttpRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Bundle\\HttpRouting\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
