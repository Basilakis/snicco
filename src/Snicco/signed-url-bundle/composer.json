{
  "name": "sniccowp/signed-url-bundle",
  "description": "Provides ready to use backend storage specific to WordPress when using sniccowp/signed-url",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "sniccowp/signed-url": "dev-master",
    "sniccowp/signed-url-wp": "dev-master",
    "sniccowp/signed-url-middleware": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\SignedUrlBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SignedUrlBundle\\": "tests"
    }
  }
