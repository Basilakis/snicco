{
  "name": "sniccowp/pimple-brige",
  "description": "Provides an adapter for the pimple DI container for sniccowp/core",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/simple-cache": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "cache/array-adapter": "^1.0.0",
    "sniccowp/session": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bridge\\SessionPsr16\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bridge\\SessionPsr16\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}