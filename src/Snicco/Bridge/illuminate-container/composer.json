{
  "name": "sniccowp/illuminate-container-brige",
  "description": "Provides an adapter for the Laravel DI container for sniccowp/core",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/container": "^8.0.0",
    "sniccowp/core": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bridge\\IlluminateContainer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bridge\\IlluminateContainer\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}