{
    "name": "sniccowp/signed-url-psr16",
    "description": "Use any psr16 cache with sniccowp/signed-url",
    "license": "MIT",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "psr/simple-cache": "^1.0.0"
    },
    "require-dev": {
        "cache/array-adapter": "^1.0.0",
        "phpunit/phpunit": "9.5.13",
        "sniccowp/signed-url": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Bridge\\SignedUrlPsr16\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Bridge\\SignedUrlPsr16\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
