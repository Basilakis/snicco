{
  "name": "sniccowp/signed-url-middleware",
  "description": "Psr15 middleware integration for sniccowp/signed-url.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/log": "^1.0.0",
    "psr/http-message": "^1.0.0",
    "psr/http-server-middleware": "^1.0.0",
    "sniccowp/signed-url": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "sniccowp/testing-bundle": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bridge\\SignedUrlMiddleware\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bridge\\SignedUrlMiddleware\\Tests\\": "tests"
    }
  }
